{
  "name": "trillo-csstutorialproject2",
  "version": "1.0.0",
  "description": "Css tutorial project 2",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass scss/main.scss css/style.css -w",

    "compile:sass": "sass scss/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.css --style=compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnshumanMahato/Trillo-cssTutorialProject2.git"
  },
  "keywords": [
    "flexbox",
    "trillio",
    "css-tutorial"
  ],
  "author": "Anshuman Mahato",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AnshumanMahato/Trillo-cssTutorialProject2/issues"
  },
  "homepage": "https://github.com/AnshumanMahato/Trillo-cssTutorialProject2#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.4",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.13"
  },
  "dependencies": {}
}
